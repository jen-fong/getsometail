
    <div class="row dogRow">
        <div class="col-md-12 dogThumbnails">
            {{#each dogs }}
                <a href="#" class="img-circle dogCircle ">
                    <img class="img-responsive " src="../assets/images/thumbnails/{{ image }}.png" alt={{ breed }}>
                    <div>       
                        <button type="button" id="dogButton" class="btn btn-primary-topNav dog hvr-grow" data-breed={{ breed }} data-toggle="modal" data-target="#modal-2">{{ breed }}</button>
                    </div>
                </a>
            {{/each }}  
        </div>
     </div>

   

    <div class="row bottom"> 
        <div class="col-md-7 descriptionDiv">
           <div id="title" class="span1">{{ girlHeading.heading1}} <span>{{ girlHeading.heading2 }}</span>  {{girlHeading.heading3}}
                <p>{{ girl.description }}</p>
            </div>
        </div>
    </div>

