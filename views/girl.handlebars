{{#each dogs }}
    <div class="col-md-2">
            <a href="#" class="img-circle">
                <img class="dog" src="../assets/images/thumbnails/{{ image }}.png" data-breed={{ breed }} alt={{ breed }}>
                <div class="caption text-center">
            
            <button type="button" class="btn btn-primary-topNav hvr-float-shadow" data-toggle="modal" data-target="#modal-2">{{ breed }}</button>
        <div class="modal fade" id="modal-2">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
